menuPanel.Size = isMobile and UDim2.new(0, 100, 0, 164) or UDim2.new(0, 86, 0, 140)
menuPanel.Position = isMobile and UDim2.new(0, 50, 0, 10) or UDim2.new(0, 45, 0, 10)
